    package="com.app.clientevulcar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClienteVulcar">
        <activity android:name=".Activitys.EditAddress"></activity>
        <activity android:name=".Activitys.MyAddress" />
        <activity android:name=".Activitys.Register2" />
        <activity android:name=".Activitys.Profile" />
        <activity android:name=".Activitys.Requests" />
        <activity android:name=".Activitys.Search" />
        <activity android:name=".Activitys.Home" />
        <activity android:name=".Activitys.Register" />
        <activity android:name=".Activitys.Login" />
        <activity android:name=".Activitys.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>